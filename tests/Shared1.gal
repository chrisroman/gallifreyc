class Foo {
    public int x;
    public Foo() {
        x = 0;
    }
    public Foo(int x) {
        this.x = x;
    }
    public void setX(int x) {
        this.x = x;
    }
}

public class Hello {
    public static void main(local String[] args) {
        shared[MyRestriction] Foo foo = new Foo();
        foo.setX(5);
        System.out.println(foo.x);
    }
}
